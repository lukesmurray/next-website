{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next-remote-watch",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "yarn reset:db && yarn generate && next build",
    "start": "next start",
    "test": "jest",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "generate:graphqltypes": "graphql-codegen --config codegen.yml",
    "generate:graphqltypes:watch": "yarn generate:graphqltypes --watch",
    "generate:prisma": "yarn prisma generate && yarn ts-node ./lib/scripts/generateSchema.ts",
    "generate:prisma:watch": "chokidar \"./prisma/schema.prisma\" -c 'yarn generate:prisma'",
    "generate:markdown": "yarn prisma db seed --preview-feature && yarn refresh",
    "generate:markdown:watch": "chokidar \"./content/**/*.{md,mdx}\" -c 'yarn generate:markdown'",
    "watch": "yarn reset:db && yarn generate && concurrently --kill-others \"yarn dev\" \"yarn generate:graphqltypes:watch\" \"yarn generate:prisma:watch\" \"yarn generate:markdown:watch\"",
    "generate": "yarn generate:prisma && yarn generate:graphqltypes && yarn generate:markdown",
    "refresh": "curl -s --header \"Content-Type: application/json\" --request POST http://localhost:3000/__next_reload || true",
    "reset:db": "yarn prisma db push --preview-feature --force-reset",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/server": "^11.0.0",
    "@emotion/styled": "^11.1.5",
    "@tippyjs/react": "^4.2.5",
    "next": "10.0.9",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^4.2.0",
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.12",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-plugin": "^11.2.0",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@prisma/client": "^2.19.0",
    "@tailwindcss/typography": "^0.4.0",
    "@types/graphql-fields": "^1.3.3",
    "@types/hast": "^2.3.1",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/tailwindcss": "^2.0.2",
    "apollo-server-micro": "^2.21.2",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-macros": "^3.0.1",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^6.0.0",
    "file-loader": "^6.2.0",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.9.0",
    "gray-matter": "^4.0.2",
    "hast-util-classnames": "^1.0.2",
    "image-size": "^0.9.7",
    "jest": "^26.6.3",
    "micro": "^9.3.4",
    "next-mdx-remote": "^2.1.3",
    "next-remote-watch": "^0.3.0",
    "patch-package": "^6.4.7",
    "postcss": "^8.2.8",
    "prisma": "^2.19.0",
    "reflect-metadata": "^0.1.13",
    "remark-footnotes": "2.0.0",
    "remark-numbered-footnotes": "^3.0.0",
    "remark-prism": "^1.3.4",
    "remark-wiki-link": "0.0.4",
    "tailwindcss": "^2.0.4",
    "ts-node": "^9.1.1",
    "twin.macro": "^2.3.0",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.12.0",
    "typescript": "^4.2.3"
  }
}
