{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next-remote-watch",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate:prisma": "prisma generate",
    "generate:markdownwatch": "chokidar \"./content/**/*.{md,mdx}\" -c 'yarn prisma db seed --preview-feature && curl -s --header \"Content-Type: application/json\" --request POST http://localhost:3000/__next_reload' ",
    "generate:watch": "concurrently --kill-others \"yarn generate:graphql --watch\" \"yarn generate:prisma --watch\" \"yarn generate:markdownwatch\"",
    "generate": "yarn generate:prisma && yarn generate:graphql",
    "update-data": "yarn generate:prisma && prisma db push --preview-feature && prisma db seed --preview-feature"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/server": "^11.0.0",
    "@emotion/styled": "^11.1.5",
    "@tailwindcss/typography": "^0.4.0",
    "next": "9.5.5",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.12",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-plugin": "^11.2.0",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@prisma/client": "^2.19.0",
    "@types/graphql-fields": "^1.3.3",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "apollo-server-micro": "^2.21.2",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-macros": "^3.0.1",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^6.0.0",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.9.0",
    "gray-matter": "^4.0.2",
    "jest": "^26.6.3",
    "micro": "^9.3.4",
    "next-remote-watch": "^0.3.0",
    "postcss": "^8.2.8",
    "prisma": "^2.19.0",
    "reflect-metadata": "^0.1.13",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "tailwindcss": "^2.0.4",
    "ts-node": "^9.1.1",
    "twin.macro": "^2.3.0",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.12.0",
    "typescript": "^4.2.3"
  }
}
