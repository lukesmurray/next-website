{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next-remote-watch",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev",
    "build": "yarn reset:db && yarn generate && next build",
    "start": "next start",
    "test": "jest",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "generate:graphqltypes": "graphql-codegen --config codegen.yml",
    "generate:graphqltypes:watch": "yarn generate:graphqltypes --watch",
    "generate:prisma": "yarn prisma generate && yarn ts-node ./lib/scripts/generateSchema.ts",
    "generate:prisma:watch": "chokidar \"./prisma/schema.prisma\" -c 'yarn generate:prisma'",
    "generate:markdown": "yarn prisma db seed && yarn refresh",
    "generate:markdown:watch": "chokidar \"./content/**/*.{md,mdx}\" -c 'yarn generate:markdown'",
    "watch": "yarn reset:db && yarn generate && concurrently --kill-others \"yarn dev\" \"yarn generate:graphqltypes:watch\" \"yarn generate:prisma:watch\" \"yarn generate:markdown:watch\"",
    "generate": "yarn generate:prisma && yarn generate:graphqltypes && yarn generate:markdown",
    "refresh": "curl -s --header \"Content-Type: application/json\" --request POST http://localhost:3000/__next_reload || true",
    "reset:db": "yarn prisma db push --force-reset",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@tippyjs/react": "^4.2.5",
    "next": "12.0.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-spring": "^9.2.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.19",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@graphql-codegen/cli": "2.2.2",
    "@graphql-codegen/typescript": "2.3.0",
    "@graphql-codegen/typescript-operations": "2.2.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.0",
    "@prisma/client": "3.3.0",
    "@tailwindcss/typography": "^0.4.1",
    "@types/graphql-fields": "^1.3.3",
    "@types/hast": "^2.3.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/tailwindcss": "^2.0.4",
    "apollo-server-micro": "^3.4.0",
    "autoprefixer": "^10.2.6",
    "babel-jest": "^27.0.2",
    "babel-plugin-macros": "^3.1.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.2.0",
    "feed": "^4.2.2",
    "file-loader": "^6.2.0",
    "graphql": "^15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.9.3",
    "gray-matter": "^4.0.3",
    "hast-util-classnames": "^2.0.0",
    "image-size": "^1.0.0",
    "jest": "^27.0.3",
    "micro": "^9.3.4",
    "next-mdx-remote": "^3.0.8",
    "next-remote-watch": "^1.0.0",
    "next-transpile-modules": "^9.0.0",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.0",
    "prisma": "3.4.0",
    "reflect-metadata": "^0.1.13",
    "remark": "^14.0.1",
    "remark-footnotes": "4.0.1",
    "remark-numbered-footnotes": "^3.0.0",
    "remark-prism": "^1.3.6",
    "remark-wiki-link": "0.0.4",
    "tailwindcss": "^2.1.4",
    "ts-node": "^10.0.0",
    "twin.macro": "^2.4.2",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.16.4",
    "typescript": "^4.3.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
